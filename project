#include<iostream>
using namespace std;
void separateDecimal(double num)
{
	int integerPart=(int)num;
	double deci,alpart=num-integerPart;
	cout<<"integer part:"<<"integerpart"<<'end';
	cout<<"Decimal part:"<<"desimalpart"<<'end';
}
int main()
{
	double average;
	cout<<"Enter your average:";
	cin>>average;
	separateDecimal(average);
	
	return 0;
}
